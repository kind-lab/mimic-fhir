# Example .env file for testing

# SQL connection
# SQL connection
export SQLUSER="postgres"
export SQLPASS="postgres"
export DBNAME_MIMIC="mimic"
export DBNAME_HAPI="hapi_r4"
export DBHOST="localhost"
export PGPORT=5432

# FHIR Server Setup
export FHIR_SERVER="http://localhost:8080/fhir/"
export MIMIC_TERMINOLOGY_PATH="/path/to/terminology/mimic-profiles/input/resources/"

# Local MIMIC-FHIR files
export MIMIC_JSON_PATH="/path/to/output/jsons/"
export FHIR_BUNDLE_ERROR_PATH="/path/to/logs/bad_bundles"
export MIMIC_FHIR_LOG_PATH="/path/to/logs/"

# Validation
export FHIR_VALIDATOR="GCP"

# Java Validator
export JAVA_VALIDATOR="/path/to/validator/validator_cli.jar"
export MIMIC_IG_PATH="/path/to/implementation_guide/package.tgz"

# Google Cloud Platform settings
export GCP_PROJECT="project-name"
export GCP_LOCATION="us-central1"
export GCP_TOPIC="mimic-fhir-topic"
export GCP_BUCKET="mimic-fhir"
export GCP_DATASET="mimic-fhir-dataset"
export GCP_FHIRSTORE="mimic-fhir-store"
export GCP_EXPORT_FOLDER="mimic-iv-fhir-v2-export"
export GCP_TOPIC_PATIENT_EVERYTHING='patient-everything'
